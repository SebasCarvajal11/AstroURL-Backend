<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="012-add-last-accessed-at-to-urls" author="dev.astrourl">
        <comment>Adds last_accessed_at to urls for efficient reporting.</comment>
        <addColumn tableName="urls">
            <column name="last_accessed_at" type="TIMESTAMP"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>